{
	"name": "code-plus",
	"displayName": "Code Plus",
	"description": "Search for errors on Stack Overflow and load custom code snippets. ",
	"repository": {
		"type": "git",
		"url": "git://github.com/dtaneja123/codeplus.git"
	  },
	"version": "1.0.1",
	"icon": "code-plus.png",
	"publisher": "devashishTaneja",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:code-plus.searchOverflow",
		"onCommand:code-plus.loadSnippet"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "code-plus.searchOverflow",
				"title": "Search Overflow"
			},
			{
				"command": "code-plus.loadSnippet",
				"title": "Load Snippet"
			}
		],
		"menus": {
            "editor/context": [
                {
                    "command": "code-plus.searchOverflow",
                    "group": "z_commands",
                    "when": "editorTextFocus"
                },
				{
                    "command": "code-plus.loadSnippet",
                    "group": "z_commands",
                    "when": "editorTextFocus"
                }

			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"node-fetch": "^2.6.1"
	}
}
